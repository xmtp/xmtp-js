{
  "author": "XMTP Labs <eng@xmtp.com>",
  "bugs": {
    "url": "https://github.com/xmtp/xmtp-js/issues"
  },
  "dependencies": {
    "@xmtp/content-type-reaction": "^2.0.2",
    "@xmtp/content-type-remote-attachment": "^2.0.2",
    "@xmtp/content-type-reply": "^2.0.2",
    "@xmtp/content-type-text": "^2.0.2",
    "@xmtp/node-sdk": "^4.1.1",
    "uint8arrays": "^5.1.0",
    "viem": "^2.37.6"
  },
  "description": "XMTP Agent SDK for interacting with XMTP networks",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@types/node": "^22.18.4",
    "@vitest/coverage-v8": "^3.2.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*"
  ],
  "homepage": "https://github.com/xmtp/xmtp-js",
  "keywords": [
    "agents",
    "javascript",
    "js",
    "messaging",
    "node",
    "nodejs",
    "typescript",
    "web3",
    "xmtp"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@xmtp/agent-sdk",
  "packageManager": "yarn@4.5.0",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "directory": "sdks/agent-sdk",
    "type": "git",
    "url": "git+https://git@github.com/xmtp/xmtp-js.git"
  },
  "scripts": {
    "build": "yarn clean:dist && tsc -p tsconfig.build.json && tsc-alias && attw --pack . --ignore-rules cjs-resolves-to-esm",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:dist",
    "clean:dist": "rm -rf dist",
    "demo": "tsx src/demo.ts",
    "dev": "tsx --watch src/main.ts",
    "gen:keys": "tsx src/bin/generateKeys.ts",
    "start": "yarn demo",
    "test": "yarn typecheck && vitest run --typecheck",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.0.13"
}
