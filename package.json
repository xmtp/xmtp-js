{
  "name": "@xmtp/xmtp-js",
  "version": "0.0.0-development",
  "description": "XMTP client SDK for interacting with XMTP networks.",
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "umd:main": "dist/umd/src/index.js",
  "types": "dist/types/src/index.d.js",
  "scripts": {
    "build": "npm run build:proto && npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:proto": "npm run clean:proto && buf generate && npm run build:waku-proto",
    "build:waku-proto": "cd ./src/proto/waku && buf generate",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "build:docs": "rm -rf docs && mkdir -p tmp && cp README.md tmp/ && sed -i.bak '/badge.svg/d' tmp/README.md && typedoc --excludePrivate --readme tmp/README.md src/index.ts",
    "clean": "node tools/cleanup && npm run clean:proto",
    "clean:proto": "rm -rf src/proto/*.ts src/proto/**/*.ts",
    "package": "npm pack",
    "prepare": "npm run install:buf && npm run build",
    "install:buf": "./script/buf.sh || true",
    "test": "npm run test:node",
    "test:node": "jest --no-cache --runInBand --env='node' --forceExit",
    "test:jsdom": "jest --no-cache --runInBand --env='./jest.jsdom.env.js'",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "lint": "prettier --check . && eslint .",
    "autolint": "prettier --write . && eslint --fix .",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "xmtp",
    "messaging",
    "web3",
    "sdk",
    "js",
    "javascript",
    "node",
    "nodejs"
  ],
  "author": "XMTP Labs <eng@xmtp.com>",
  "license": "MIT",
  "homepage": "https://github.com/xmtp/xmtp-js",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:xmtp/xmtp-js.git"
  },
  "bugs": {
    "url": "https://github.com/xmtp/xmtp-js/issues"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "dependencies": {
    "@noble/secp256k1": "^1.5.2",
    "@stardazed/streams-polyfill": "^2.4.0",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "ethers": "^5.5.3",
    "it-length-prefixed": "^7.0.1",
    "it-pipe": "^2.0.3",
    "js-waku": "^0.18.0",
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/bl": "^5.0.2",
    "@types/callback-to-async-iterator": "^1.1.4",
    "@types/jest": "^27.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.0",
    "node-localstorage": "^2.2.1",
    "prettier": "^2.4.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-proto": "^1.104.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
